@charset "utf-8";
/* CSS Document */

.main_wrap {clear:both;float:left;width:100%;height:1000px}
.main_left {float:left;width:29%;height:1000px;border-right:1px solid #444}
.main_middle {float:left;width:42%;height:1000px;position:relative}
.main_right {float:left;width:29%;height:1000px;border-left:1px solid #444}

h2.main_title {display:inline-block;width:100%;height:40px;font-size:20px;font-weight:400}
h2.main_title a {display:inline-block;width:100%;height:40px;background:url("/dist/images/main/go_arrow_icon.png") no-repeat right 2px}
.maintxt_sm {font-size:14px;line-height:160%;}
.bg_none {background:0 none !important}

.covid_19 {width:100%;height:220px;padding:20px;background:#2c95c6 url("/dist/images/main/ico_mask.png") no-repeat 95% 20px; cursor: pointer;}
.covid_sum {clear:both;display:inline-block;width:100%;height:55px;text-align:center;margin-top:17px}
.covid_sum dt {display:inline-block;min-width:100px;height:26px;border-radius:30px;background-color:#2c80a7;padding:4px 12px 0 12px;margin:12px 10px 0 0}
.covid_sum dd {display:inline-block;font-size:44px;font-weight:500}
.txt18 {vertical-align:bottom;font-size:18px;line-height:140% !important;font-weight:400;padding-left:3px}
.covid_bottom {clear:both;display:inline-block;width:100%;margin-top:24px}
.covid_box {display:inline-block;width:32%;height:44px;border-radius:5px;background-color:#2a2a2a;margin-right:1%;padding:9px 11px 0 11px}
.covid_bottom .covid_box:last-child {margin-right:0 !important}
.covid_box dt {display:inline-block;width:30%;font-size:14px;font-weight:300 !important;margin-top:5px}
.covid_box dd {display:inline-block;width:67%;font-size:24px;text-align:right;font-weight:500}

.population {width:100%;height:230px;padding:20px;border-top:1px solid #444}
.sex_population {float:left;width:100%;height:96px;}
.all_people {float:left;width:53.3%;margin-right:1.4%;height:96px;border-radius:5px;background-color:#222;padding:22px 20px 0 20px}
.all_people dt {float:left;width:40%;height:56px;background:url(/dist/images/main/ico_all.png) no-repeat 0 13px;padding:16px 0 0 38px}
.all_people dd {float:left;width:60%;text-align:right;font-size:44px;font-weight:500}
.sex_desc {float:left;width:45.3%;}
.people_man {float:left;width:100%;height:44px;border-radius:5px;background-color:#222;padding:11px 20px 0 20px}
.people_man dt {float:left;width:40%;height:34px;background:url(/dist/images/main/ico_man.png) no-repeat 0 4px;padding:2px 0 0 17px}
.people_man dd, .people_woman dd {float:left;width:60%;text-align:right;font-size:20px;font-weight:500}
.people_woman {float:left;width:100%;height:44px;border-radius:5px;background-color:#222;margin-top:8px;padding:11px 20px 0 20px}
.people_woman dt {float:left;width:40%;height:34px;background:url(/dist/images/main/ico_woman.png) no-repeat 0 4px;padding:2px 0 0 17px}
.move_population {clear:both;display:inline-block;width:100%;text-align:center;margin-top:23px}
.flow_move {display:inline-block;width:100%;text-align:center}
.flow_move li {display:inline-block;font-size:24px;font-weight:500;vertical-align:middle}
.flow_move li span {display:inline-block;text-align:center;border-radius:3px;padding:1px 5px 3px 5px;font-size:13px;font-weight:300;vertical-align:top;margin:4px 3px 0 0;line-height:1}
.flow_in {border:1px solid #38dbe5;color:#38dbe5}
.flow_out {border:1px solid #e9a233;color:#e9a233}
.flow_ap {border:1px solid #fff;color:#fff}
.num_sign {padding:0 10px 0 10px}

.weather {width:100%;height:198px;padding:20px;border-bottom:1px solid #444}
.w_box {clear:both;display:inline-block;width:23%;height:113px;margin-right:1%;border-radius:5px;background-color:#222;padding:15px}
.w_box dt {width:100%;height:28px;font-size:15px;text-align:center;border-bottom:1px solid #444}
.w_box dd {width:100%;font-size:24px;font-weight:500;text-align:center;padding-top:18px;line-height:120%}
.weather .w_box:last-child {margin-right:0 !important}
.weather .w_box dd span {display:inline-block;width:60px;height:30px;font-size:13px;padding-left:33px}
.weather_1 {color:#00fe0c;background:url("/dist/images/main/weather_1.png") no-repeat 0 0;padding-top:8px}
.weather_2 {color:#78ff00;background:url("/dist/images/main/weather_2.png") no-repeat 0 0;padding-top:8px}
.weather_3 {color:#ffa800;background:url("/dist/images/main/weather_3.png") no-repeat 0 0;padding-top:8px}
.weather_4 {color:#ff2828;background:url("/dist/images/main/weather_4.png") no-repeat 0 0;line-height:110%}

.st_blue {border:1px solid #00aeff !important;color:#00aeff !important}
.st_green {border:1px solid #22d42f !important;color:#22d42f !important}

.apply_minwon {width:100%;height:165px;padding:20px;border-bottom:1px solid #444}
.apply_state {clear:both;display:inline-block;width:100%}
.apply_state .state_bx {display:inline-block;width:24.5%;height:85px;border-right:1px dotted #777;padding-top:8px}
.apply_state .state_bx:last-child {border-right:0 none !important}
.state_bx dt {display:inline-block;width:100%;text-align:center}
.state_bx dt span {display:inline-block;min-width:68px;height:22px;border-radius:5px;font-size:14px;text-align:center;padding:2px 8px 0 7px;font-weight:300}
.state_bx dd {display:inline-block;width:100%;margin-top:14px;text-align:center;font-size:24px;font-weight:500}

.news {width:100%;height:465px;padding:20px 20px 0px 20px}/* 초기 height : 325 */
.main_list {clear:both;display:inline-block;width:100%;overflow-y: scroll;height: 100%;}
.main_list li {display:inline-block;width:100%;border-bottom:1px dotted #777;padding:12px 0 9px 0;overflow:hidden;text-overflow:ellipsis;white-space:nowrap}
.main_list li:first-child {padding-top:10px !important}
.main_list li:last-child {border-bottom:0 none !important}
.news_date {font-size:14px;color:#acacac;line-height:200%;}

.bg_left {width:50%;height:999px;background:url("/dist/images/main/bg_left.png") repeat-y 0 0;z-index:1;position:absolute;top:0;left:0}
.bg_right {width:50%;height:999px;background:url("/dist/images/main/bg_right.png") repeat-y right top;z-index:1;position:absolute;top:0;right:0}
.middle_all {position:absolute;top:0;left:0;clear:both;width:100%;z-index:5;}
.roll_menu {position:absolute;top:34px;left:25px;width:150px;height:180px;overflow-y:auto;z-index:29;padding-right:8px}
.mid_menu {clear:both;display:inline-block;width:100%;}
.mid_menu li {display:inline-block;width:100%;height:28px;margin-bottom:8px}
.mid_menu li a {display:inline-block;width:100%;height:28px;border-radius:30px;background-color:#353535;box-shadow:1px 1px 1px 2px #222;text-align:center;padding-top:6px;font-size:14px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap}
.mid_menu li a.on {background-color:#009cff !important;color:#2a2a2a !important}
.title_area {width:100%;text-align:center;padding-top:60px}
.title_center {width:100%;height:48px;text-align:center;background:url("/dist/images/main/title_shadow.png") no-repeat center bottom;font-size:24px;font-weight:400}
.cont_title {width:100%;text-align:center;font-size:20px;color:#009cff;font-weight:400;line-height:120%}

.map_area {clear:both;width:100%;height:790px;margin-top:20px;padding:20px 50px 0 50px;position:relative;background:url(/dist/images/main/cmm_dong_bndr.svg) no-repeat center 20px/90%}

.mini_pop {position:absolute;top:470px;right:55px;width:178px;min-height:169px;background-color:#2a2a2a;border:1px solid #717171;box-shadow:1px 1px 1px 1px #222;padding:12px;z-index:10}
.mini_all {position:relative;width:100%}
.mini_close {position:absolute;top:0;right:0;width:11px;height:11px;background:url("/dist/images/main/mini_close.png") no-repeat 2px 2px;font-size:0}
.mini_title {width:100%;text-align:center;font-size:14px;line-height:160%}
.font20 {font-size:20px;font-weight:500;vertical-align:top}
.mini_desc {width:100%;margin-top:7px;border-top:1px solid #444;font-size:13px;padding-top:10px}
.mini_desc li {width:100%;background:url(/dist/images/common/bullet_3_3.png) no-repeat 0 8px;padding:0 0 2px 8px;line-height:140%}
.map_remark {position:absolute;bottom:0;left:0;display:inline-block;width:100%;text-align:center;margin-top:10px}
.map_remark .main_rm {display:inline-block;min-width:300px;margin:0 auto;text-align:center}
.main_rm li {display:inline-block;height:68px;text-align:center;padding:0 7px 0 7px;position:relative;min-width:80px}
.main_rm li .rm_text {width:100%;text-align:center;position:absolute;bottom:0;left:0;font-size:12px;line-height:180%}
.main_rm li span {width:100%;text-align:center;position:absolute;bottom:25px;left:0}
.rm_lv1 img {width:32px}.rm_lv2 img {width:38px}.rm_lv3 img {width:44px}.rm_lv4 img {width:49px}

/*20210907 추가*/
