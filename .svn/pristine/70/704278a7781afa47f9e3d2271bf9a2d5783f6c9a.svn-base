@charset "utf-8";
/* CSS Document */

.s_minwon01 {clear:both;display:inline-block;width:100%;height:296px}
.s_minwon02 {clear:both;display:inline-block;width:100%;height:565px}
.s_minwon03 {clear:both;display:inline-block;width:100%;height:901px;overflow-y: scroll;}

.apply_all {display:inline-block;width:100%;height:264px}
.apply_box {display:inline-block;width:49%;margin-right:1.5%}
.apply_all .apply_box:last-child, .apply_box .apply_num:last-child, .keyword_wrap .keyword_area:last-child {margin-right:0 !important}
.minwon_title {width:100%;height:50px;border-radius:10px;background-color:#232323;text-align:center;font-size:18px;padding-top:15px}
.apply_num {display:inline-block;width:48.5%;height:192px;margin:12px 2% 0 0;border-radius:10px;background-color:#232323;padding:10px 24px 0 24px}
.apply_num dt {display:inline-block;width:100%;height:45px;border-bottom:1px dotted #595959;text-align:center;padding-top:10px;font-size:18px;}
.apply_num dd {display:inline-block;width:100%;padding-top:30px;}
.dd_txt {display:inline-block;width:50%;padding-top:20px}
.dd_num {display:inline-block;width:48%;font-size:54px;text-align:right;font-weight:600}

@media all and (max-width:1366px) {
	.apply_num dd {padding-top:20px}
	.dd_txt {width:100%;padding-top:0;text-align:center}
	.dd_num {width:100%;padding-top:10px;text-align:center}
	.kw_100 dt {font-size:16px !important}
	.kw_100 dd {font-size:20px !important}
	.kw_50 dt {font-size:12px !important}
	.kw_50 dd {font-size:14px !important}
}

.dong_apply_all {display:inline-block;width:100%;height:520px;overflow-y:auto}
.dong_unit {display:inline-block;width:285px;height:109px;background:url("/dist/images/minwon/dong_bg.png") no-repeat 0 0;margin:0 15px 18px 0}
.unit_con {clear:both;float:left;width:100%;height:109px}
.unit_con li {float:left}
.pic_dong {width:120px;height:109px}
.dong_name {width:80px;font-size:18px;font-weight:400;margin-top:17px}
.dong_name .klein {width:100%;font-size:12px;color:#7f7f7f;line-height:170%}
.sum_up {width:85px;font-size:24px;text-align:right;margin-top:19px;font-weight:600;padding-right:17px}

.news_wrap {display:inline-block;width:100%;height:860px}
.news_list_all, .list_news li {display:inline-block;width:100%}
.list_news {display:inline-block;width:100%;border-top:1px solid #616161}
/* 20220721예비수정 */
/* .list_news li a {display:inline-block;width:100%;border-bottom:1px solid #616161;padding:10px 15px 12px 15px;line-height:130%} */
.list_news li a {display:inline-block;width:100%;border-bottom:1px solid #616161;padding:10px 15px 12px 15px;}
.list_news li a:hover {background-color:#373737}
.list_news li a.on {color:#ffffff;background:#373737 url("/dist/images/minwon/ico_check_white.png") no-repeat 97% 50%}
.list_news li a span.n_origin {font-size:14px;color:#acacac;line-height:150%}
.desc_tip {display:inline-block;width:100%;font-size:14px;color:#7f7f7f;background:url("/dist/images/minwon/ico_check_gray.png") no-repeat 8px 4px;padding-left:25px;line-height:120%;margin-top:20px}
.news_desc {width:100%;height:340px;overflow-y:scroll;background-color:#232323;margin-top:14px;padding:15px;font-size:14px;line-height:160%}

.anal_area01 {clear:both;display:inline-block;width:100%;height:246px;position:relative}
.anal_area02 {clear:both;display:inline-block;width:100%;height:615px}
.anal_area03 {clear:both;display:inline-block;width:100%;height:210px;position:relative}
.anal_area04 {clear:both;display:inline-block;width:100%;height:363px}

.keyword_wrap {display:flex;width:100%;height:202px;overflow-y:auto}
.keyword_tip {position:absolute;top:0;right:10px;font-size:12px;color:#d1d1d1}
.keyword_wrap .keyword_area {clear:both;float:left;width:9.5%;height:202px;margin-right:0.5%;position:relative}
.box_num {position:absolute;top:0;left:38%;width:40px;height:40px;border-radius:50%;background-color:#fff;text-align:center;font-size:18px;font-weight:600;padding-top:9px;color:#2a2a2a}
.keyword_box {float:left;width:100%;height:182px;margin-top:20px;border-radius:10px;background-color:#232323;padding:30px 15px 0 15px;cursor:pointer}
.kw_100 {float:left;width:100%;height:70px;border-bottom:1px dotted #595959}
.kw_100 dt {width:100%;font-size:18px;text-align:center;margin-bottom:8px}
.kw_100 dd {width:100%;font-size:24px;text-align:center;font-weight:600;color:#fff600}
.add_unit {font-size:14px;line-height:210%;font-weight:400;padding-left:3px;color:#fff !important}
.kw_50 {float:left;width:50%;height:65px;padding-top:12px}
.kw_50 dt {width:100%;text-align:center;margin-bottom:8px;font-size:14px}
.kw_50 dd {width:100%;text-align:center;font-size:18px;font-weight:600}
.dotline_r {border-right:1px dotted #595959}
.keyword_area div.on {border:2px solid #fff}
.search_kw {position:absolute;top:0;right:1%}
.search_kw ul li {display:inline-block;vertical-align:top}
.kw_tbl {width:100%;height:581px;}
.kw_list {width:100%;height:545px;overflow-y:scroll}

.grp_581 {width:100%;height:581px}
.grp_180 {width:100%;height:180px}
.grp_333 {width:100%;height:333px}


/*추가*/
.newList:hover, .newList:active {
	color:#ffffff;
}


